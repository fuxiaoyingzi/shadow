<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    tools:context="com.example.administrator.shadowapplication.timer.CustomTimer1Activity">
    <TextView
        android:background="@color/md_pink_50"
        android:padding="20dp"
        android:id="@+id/tvStartCount"
        android:text="开始计时"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:background="@color/md_pink_50"
        android:padding="20dp"
        android:layout_marginTop="50dp"
        android:id="@+id/tvToCountTimer2"
        android:text="跳转到countTime2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</LinearLayout>
